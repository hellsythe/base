    "keywords": [
        "laravel",
        "package",
        "base",
        "api generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Camilo Antonio Rodríguez Cruz",
            "email": "camilo@sdkconsultoria.com",
            "homepage": "https://sdkconsultoria.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "intervention/image": "^2.7",
        "spatie/laravel-permission": "^6.1",
        "laravel/sanctum": "^3.0"
    },
    "require-dev": {
